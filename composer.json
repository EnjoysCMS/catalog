{
  "name": "enjoyscms/catalog",
  "description": "Module Catalog for EnjoysCMS",
  "type": "enjoyscms-module",
  "require": {
    "php": ">=8.0",
    "enjoyscms/core": "^4.0",
    "enjoyscms/admin": "^4.0",
    "gedmo/doctrine-extensions": "^3.0",
    "beberlei/doctrineextensions": "^1.3",
    "jbroadway/urlify": "^1.2",
    "codeguy/upload": "^1.3",
    "intervention/image": "^2.5",
    "enjoyscms/elfinder": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "EnjoysCMS\\Module\\Catalog\\": "src/",
      "Build\\": "build/"
    }
  },
  "extra": {
    "module-name": "Каталог",
    "admin-links": {
      "catalog/admin/category": "Категории",
      "catalog/admin/products": "Продукты",
      "catalog/admin/pricegroup": "Группы цен",
      "catalog/admin/setting": "Настройки"
    }
  },
  "scripts": {
    "assets-install": "cd ./modules/catalog && yarn install"
  },
  "require-dev": {
    "enjoys/dotenv": "^2.0",
    "symfony/cache": "^6.0"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "oomphinc/composer-installers-extender": true
    }
  }
}
